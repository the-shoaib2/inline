        ],
        "types": [
            "type\\s+\\w+\\s*="
        ],
        "variables": [
            "const\\s+\\w+",
            "let\\s+\\w+",
            "var\\s+\\w+"
        ],
        "comments": [
            "//.*",
            "/\\*[\\s\\S]*?\\*/"
        ]
    },
    "javascript": {
        "commentPrefix": "//",
