        ],
        "functions": [
            "function\\s+\\w+\\s*\\([^)]*\\)",
            "const\\s+\\w+\\s*=\\s*\\([^)]*\\)\\s*=>",
            "\\w+\\s*\\([^)]*\\)\\s*{",
            "async\\s+function\\s+\\w+\\s*\\([^)]*\\)"
