{
    "typescript": {
        "commentPrefix": "//",
        "imports": [
            "^import\\s+.*\\s+from\\s+['\"].*['\"];?",
            "^import\\s+['\"].*['\"];?",
            "^import\\s+type\\s+.*\\s+from\\s+['\"].*['\"];?"
        ],
        "functions": [
            "function\\s+\\w+\\s*\\([^)]*\\)",
            "const\\s+\\w+\\s*=\\s*\\([^)]*\\)\\s*=>",
            "\\w+\\s*\\([^)]*\\)\\s*{",
            "async\\s+function\\s+\\w+\\s*\\([^)]*\\)"
        ],
        "classes": [
            "class\\s+\\w+",
            "interface\\s+\\w+",
            "type\\s+\\w+\\s*="
        ],
        "interfaces": [
