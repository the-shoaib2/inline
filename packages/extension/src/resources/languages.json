        ],
        "comments": [
            "//.*",
            "/\\*[\\s\\S]*?\\*/"
        ]
    },
    "_fallback": {
        "commentPrefix": "//",
        "imports": [],
        "functions": [],
        "classes": [],
        "variables": [],
        "comments": [
