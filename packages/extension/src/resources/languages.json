            "^import\\s+\\w+",
            "^from\\s+\\w+\\s+import\\s+.*"
        ],
        "functions": [
            "def\\s+\\w+\\s*\\([^)]*\\):",
            "async\\s+def\\s+\\w+\\s*\\([^)]*\\):"
        ],
        "classes": [
            "class\\s+\\w+"
        ],
        "variables": [
            "\\w+\\s*=\\s*.*"
        ],
        "comments": [
            "#.*",
            "\"\"\"[\\s\\S]*?\"\"\"",
            "'''[\\s\\S]*?'''"
        ]
    },
    "go": {
        "commentPrefix": "//",
        "imports": [
            "^import\\s+['\"].*['\"]",
            "^import\\s+\\([^)]*\\)"
        ],
        "functions": [
            "func\\s+\\w+\\s*\\([^)]*\\)",
            "func\\s+\\([^)]*\\)\\s+\\w+\\s*\\([^)]*\\)"
